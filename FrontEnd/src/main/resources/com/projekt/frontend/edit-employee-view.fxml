<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<ScrollPane prefHeight="420.0" prefWidth="553.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.projekt.frontend.EditEmployeeController">
    <content>
        <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="569.0" prefWidth="538.0">
            <top>
                <VBox alignment="TOP_CENTER" prefHeight="67.0" prefWidth="512.0" BorderPane.alignment="CENTER">
                    <children>
                        <Label text="System Zarządzania Pracownikami">
                            <font>
                                <Font name="System Bold" size="28.0" />
                            </font>
                        </Label>
                        <Label text="Edytuj swojego pracownika!">
                            <font>
                                <Font size="18.0" />
                            </font>
                        </Label>
                    </children>
                </VBox>
            </top>
            <center>
                <VBox alignment="TOP_CENTER" prefHeight="477.0" prefWidth="546.0" BorderPane.alignment="CENTER">
                    <children>
                        <Label text="Imię" />
                        <TextField fx:id="nameField" disable="true" editable="false" maxWidth="-Infinity" onAction="#onNameField" prefWidth="207.0" promptText="Imię" />
                        <Label text="Nazwisko" />
                        <TextField fx:id="surnameField" disable="true" editable="false" maxWidth="-Infinity" onAction="#onSurnameField" prefWidth="207.0" promptText="Nazwisko" />
                        <Label text="PESEL" />
                        <TextField fx:id="peselField" disable="true" editable="false" maxWidth="-Infinity" onAction="#onPeselField" prefWidth="207.0" promptText="PESEL" />
                        <Label text="Narodowość" />
                        <TextField fx:id="nationalityField" disable="true" editable="false" maxWidth="-Infinity" onAction="#onNationalityField" prefWidth="207.0" promptText="Narodowość" />
                        <Label text="Adres" />
                        <TextField fx:id="addressField" disable="true" editable="false" maxWidth="-Infinity" onAction="#onAddressField" prefWidth="207.0" promptText="Adres" />
                        <Label text="Email" />
                        <TextField fx:id="emailField" disable="true" editable="false" maxWidth="-Infinity" onAction="#onEmailField" prefWidth="207.0" promptText="Email" />
                        <Label text="Telefon" />
                        <TextField fx:id="phoneField" disable="true" editable="false" maxWidth="-Infinity" onAction="#onPhoneField" prefWidth="207.0" promptText="Telefon" />
                        <Label text="Data urodzenia" />
                        <TextField fx:id="birthField" disable="true" editable="false" maxWidth="-Infinity" onAction="#onBirthField" prefWidth="207.0" promptText="Data urodzenia" />
                        <Label text="Wynagrodzenie" />
                        <TextField fx:id="salaryField" maxWidth="-Infinity" onAction="#onSalaryField" prefWidth="207.0" promptText="Wynagrodzenie" />
                        <Label text="Dni wolne" />
                        <TextField fx:id="daysoffField" maxWidth="-Infinity" onAction="#onDaysoffField" prefWidth="207.0" promptText="Dni wolne" />
                        <Label text="Stanowisko" />
                        <TextField fx:id="positionField" maxWidth="-Infinity" onAction="#onPositionField" prefWidth="207.0" promptText="Stanowisko" />
                    </children>
                </VBox>
            </center>
            <bottom>
                <Button mnemonicParsing="false" onAction="#onEditEmployee" prefHeight="25.0" prefWidth="128.0" text="Edytuj pracownika" BorderPane.alignment="BOTTOM_CENTER" />
            </bottom>
        </BorderPane>
    </content>
</ScrollPane>
